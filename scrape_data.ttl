	<http://www.semanticweb.org/mbonnee/papers/hasType> <http://www.semanticweb.org/mbonnee/papers/Article> ;
	<http://www.semanticweb.org/mbonnee/papers/hasAuthor> <http://www.semanticweb.org/mbonnee/papers/Tao%2C_DC> , <http://www.semanticweb.org/mbonnee/papers/Luo%2C_Y> , <http://www.semanticweb.org/mbonnee/papers/Xu%2C_C> , <http://www.semanticweb.org/mbonnee/papers/Wen%2C_YG> , <http://www.semanticweb.org/mbonnee/papers/Gui%2C_J> ;
	<http://www.semanticweb.org/mbonnee/papers/hasKeyword> <http://www.semanticweb.org/mbonnee/papers/large_margin> , <http://www.semanticweb.org/mbonnee/papers/multi-task> , <http://www.semanticweb.org/mbonnee/papers/image_classification> , <http://www.semanticweb.org/mbonnee/papers/multi-modal> , <http://www.semanticweb.org/mbonnee/papers/Feature_extraction> ;
